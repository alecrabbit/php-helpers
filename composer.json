{
    "name": "alecrabbit/php-helpers",
    "description": "PHP helpers functions",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "nesbot/carbon": "^2.6",
        "krowinski/bcmath-extended": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "nunomaduro/collision": "^2.1",
        "symfony/var-dumper": "^4.1"
    },
    "license": "MIT",
    "autoload": {
        "files": [
            "src/Constants/constants.php",
            "src/Constants/String/constants.php",
            "src/Helpers/functions.php",
            "src/Helpers/numeric/numeric.php",
            "src/Helpers/time/time.php",
            "src/Helpers/array/array.php",
            "src/Helpers/string/string.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable":true
}
