{
    "name": "alecrabbit/php-helpers",
    "type": "library",
    "description": "PHP helpers functions",
    "keywords": [
        "array",
        "unset",
        "first",
        "format",
        "string",
        "trim",
        "zeros",
        "wrap"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "krowinski/bcmath-extended": "^4.2",
        "nesbot/carbon": "^2.11",
        "symfony/polyfill-php73": "^1.10"
    },
    "require-dev": {
        "ext-curl": "*",
        "nunomaduro/collision": "^2.1",
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2",
        "kylekatarnls/multi-tester": "^1.1"
    },
    "autoload": {
        "files": [
            "src/Helpers/Constants.php",
            "src/Helpers/Strings/Constants.php",
            "src/Helpers/functions.php",
            "src/Helpers/numeric/numeric.php",
            "src/Helpers/time/time.php",
            "src/Helpers/array/array.php",
            "src/Helpers/string/string.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AlecRabbit\\Tests\\Helpers\\": "tests/"
        }
    },
    "prefer-stable": true
}
