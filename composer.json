{
    "name": "alecrabbit/php-helpers",
    "type": "library",
    "description": "PHP helpers functions",
    "keywords": [
        "array",
        "unset",
        "first",
        "format",
        "string",
        "trim",
        "zeros",
        "protected",
        "private",
        "wrap"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "krowinski/bcmath-extended": "^4.2 || ^5.0 || ^6.0",
        "nesbot/carbon": "^2.11",
        "symfony/polyfill-php73": "^1.10"
    },
    "require-dev": {
        "ext-curl": "*",
        "nunomaduro/collision": "^4.2",
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "AlecRabbit\\": "src/"
        },
        "files": [
            "src/bootstrap.php",
            "src/Helpers/Constants.php",
            "src/Helpers/Strings/Constants.php",
            "src/Helpers/functions.php",
            "src/Helpers/numeric/numeric.php",
            "src/Helpers/time/time.php",
            "src/Helpers/array/array.php",
            "src/Helpers/string/string.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AlecRabbit\\Tests\\Helpers\\": "tests/"
        }
    },
    "prefer-stable": true
}
